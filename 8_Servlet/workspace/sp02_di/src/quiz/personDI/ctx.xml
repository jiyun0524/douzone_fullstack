<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean class="quiz.personDI.Student" id="student" name="stu" 
	p:age="어쩌구" p:name="25" p:gender="여">
	</bean>
	
	<bean class="quiz.personDI.Employee" id="employee" name="emp">
		<property name="name" value="저쩌구"></property>
		<property name="age" value="32"></property>
		<property name="gender" value="남"></property>
	</bean>

</beans>