<script type="text/javascript">
	window.onload = function() {
		 alert("innerHTML : " + document.body.innerHTML + "\n"
				+ "innerText : " + document.body.innerText);
		 
		 var list = '';
		 list += "<ul>";
		 list += "<li>Hello </li>";
		 list += "<li>Hi </li>";
		 list += "<li>Goodbye </li>";
		 list += "</ul>";
	}
</script>
