const str = `
	010-9999-3333
	jiyun9705@naver.com
	https://namu.wiki/w/%EA%B0%90%EC%9E%90
	the Wiki 1234 dog
	abbcccdddd
	`
console.log(str.match(/dog/gi))	
console.log(str.match(/\./gi))
console.log(str.match(/\.$/gim))
console.log(str.match(/d$/gim))
console.log(str.match(/^t$/gim))

