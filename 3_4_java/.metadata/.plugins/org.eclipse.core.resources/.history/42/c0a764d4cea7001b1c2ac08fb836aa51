// class.js

const yoon = {
	firstName:'kwon',
	lastName:'yoon',
	getFullName: function () {
		return `${this.lastName} ${this.firstName}`
	}
}

console.log(yoon.getFullName())

const amy = {
	firstName: 'Amy',
	lastName: 'joey',
	getFullName: function() {
		return `${this.lastName} ${this.firstName}`
	}
}


